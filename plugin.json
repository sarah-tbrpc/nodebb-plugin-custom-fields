{
  "id": "nodebb-plugin-custom-fields",
  "name": "Custom Fields",
  "description": "Adds a custom profile type field to registration.",
  "version": "1.0.1",
  "library": "./library.js",
  "hooks": [
    { "hook": "filter:register.build", "method": "addField" },
    { "hook": "filter:register.check", "method": "checkRegister" },
    { "hook": "action:user.create", "method": "saveTypeProfile" },
    { "hook": "filter:user.whitelistFields", "method": "addUserFieldWhite" }
  ],
  "nbbpm": {
    "compatibility": "^3.7.0"
  }
}
